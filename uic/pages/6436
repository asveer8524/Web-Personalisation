<script id="seminars-template" type="text/x-handlebars-template">
	<h3 class="date">{{fulldate}}</h3>
	<p><strong class="title">{{title}}</strong><br>
		Speaker: <span class="speaker">{{speaker}}</span><br>
		<span class="institution">{{institution}}</span><br>
		Host: <class="sponsor">{{sponsor}}<br></p></p>
</script>
<script id="news-template" type="text/x-handlebars-template">
	<h3>{{title}}</h3>
	<h4>{{subtitle}}</h4>
	<div class="news-content">{{{content}}}</div>
</script>
<script id="featured-template" type="text/x-handlebars-template">
	<h3>{{title}}</h3>
	<h4>{{subtitle}}</h4>
	<div class="featured-content">{{{content}}}</div>
</script>
<script>
(function($){
	var today = todaysDate(),
		totalItems = 3,
		counter = 0;
		
	for (key in seminars) {
		var day = twoDigit(seminars[key].day.toString()),
			month = twoDigit(seminars[key].month.toString());
		seminars[key].date = seminars[key].year.toString() + month + day;
		seminars[key].fulldate = decipherDate(seminars[key].date);
		seminars[key].fullmonth = getMonthFromDate(seminars[key].month);
	}
	seminars.sort(function(a,b) {
		return a.date - b.date;
	});
	var sourceSeminars = $('#seminars-template').html(),
		templateSeminars = Handlebars.compile(sourceSeminars);
	var count = 0;
	for (key in seminars) {
		seminars[key].id = key;
		if ((parseInt(today) <= parseInt(seminars[key].date)) && ++counter<=totalItems) {
			$('#seminars-listing').append(templateSeminars(seminars[key]));
			count++;
		}
	}
	if (!count) {
		$('#seminars-listing').html("<p style='margin-top: 1em'><i>There are currently no scheduled upcoming seminars.</i></p>");
	}
	
		for (key in news) {
		var day = twoDigit(news[key].day.toString()),
			month = twoDigit(news[key].month.toString());
		news[key].date = news[key].year.toString() + month + day;
		news[key].fulldate = decipherDate(news[key].date);
		news[key].fullmonth = getMonthFromDate(news[key].month);
	}
	news.sort(function(b,a) {
		return a.date - b.date;
	});
	var sourceNews = $('#news-template').html(),
		templateNews = Handlebars.compile(sourceNews);
	if (!news.length) {
		$('#newsFeed').append('<p style="margin-top: 1em"><i>There are currently no news items.</i></p>');
	}
	for (key in news) {
		$('#newsFeed').append(templateNews(news[key]));
		if (key >= 1) {
			break;
		}
	}
	
})(jQuery);
</script>
