    <script>
// Two separate script tags are needed to support browsers that lack some ES6 features. IE11.
$(document).ready(function() {
    $('.title-bar').click(function() {
      $(this).next().slideToggle();
    });
	
	$('#expand').click(function() {
		$('.title-bar').next().slideDown();
	});

	$('#collapse').click(function() {
		$('.title-bar').next().slideUp();
	});
});
</script>
<script>
$(document).ready(function() {
    Echo.channel('printer-status').listen('.new-status', (e) =>{
        var currStatus = e["status"];
        for(var key in currStatus){
            if(currStatus.hasOwnProperty(key)){
                console.log(key + "=>" + currStatus[key]);
                var docKey = "text-center " + key;
                var element = document.getElementsByClassName(key)[0];
                var upperElement = document.getElementById(key);
                if(element != null){
                    console.log(element);
                    if(currStatus[key] == 1){
                        /*
                        var htmlStatus = "<span data-tooltip aria-haspopup=\"true\" data-disable-hover=\"false\" title=\"Online\" class=\"circle status-up\"></span>";
                        var backgroundStatus = "<tr id=\"" + key +"\">";
                        upperElement.innerHTML = backgroundStatus;
                        element.innerHTML = htmlStatus;
                        */
                        var htmlStatus = "<span data-tooltip aria-haspopup=\"true\" data-disable-hover=\"false\" title=\"Online\" class=\"circle status-up\"></span>";
                        element.innerHTML = htmlStatus;
                        $("#"+key).replaceWith("<tr id=\"" + key +"\">" + $("#"+key).html()+ "</tr>")   
                    }
                    else if(currStatus[key] == -1){
                        var htmlStatus = "<span data-tooltip aria-haspopup=\"true\" data-disable-hover=\"false\" title=\"One or more issues\" class=\"circle status-down\"></span>";
                        element.innerHTML = htmlStatus;   
                        $("#"+key).replaceWith("<tr id=\"" + key +"\">" + $("#"+key).html()+ "</tr>")   
                    }
                    else if(currStatus[key] == -2){
                        var htmlStatus = "<span data-tooltip aria-haspopup=\"true\" data-disable-hover=\"false\" title=\"Printer is offline\" class=\"circle status-down\"></span>";
                        element.innerHTML = htmlStatus;   
                        $("#"+key).replaceWith("<tr class=\"status-down-bg\" id=\"" + key +"\">" + $("#"+key).html()+ "</tr>")   
                    }
                    else if(currStatus[key] == 2){
                        var htmlStatus = "<span data-tooltip aria-haspopup=\"true\" data-disable-hover=\"false\" title=\"Printer is only online. No associated Release Station\" class=\"circle status-down\">P</span>";
                        element.innerHTML = htmlStatus;   
                        $("#"+key).replaceWith("<tr id=\"" + key +"\">" + $("#"+key).html()+ "</tr>")   
                    }
                    else{
                        var htmlStatus = "<span data-tooltip aria-haspopup=\"true\" data-disable-hover=\"false\" title=\"Offline\" class=\"circle status-down\"></span>";
                        element.innerHTML = htmlStatus;   
                        $("#"+key).replaceWith("<tr class=\"status-down-bg\" id=\"" + key +"\">" + $("#"+key).html()+ "</tr>")   
                    }
                }
            }
        }
        var date = document.getElementById("date");
        date.innerHTML = e["date"];
    });
    Echo.channel('printer-status').listen('.disruption', (e) => {
    console.log(e);
    //$banner = document.querySelector('#disruption');
    $jbanner = $('#disruption');
    if (parseInt(e["status"]) == 0) {
        console.log("hide the banner!");
        //$banner.className += " hide";
        $jbanner.slideUp();
      } else {
        // We have a disruption.
        console.log("display the banner!");
        //$banner.className.replace(new RegExp('(?:^|\\s)'+ 'hide' + '(?:\\s|$)'), ' '); // Use this to support older browsers.
        $jbanner.removeClass('hide').slideDown();
      }
    });

});
</script>
