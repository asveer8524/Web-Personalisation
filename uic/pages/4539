  <script type="text/javascript">
    var QSettings = {
      
      jfeVersion: "23bd46f46bba471bd632252a8ac241709aad7416\n",
      publicPath: "//jfe-cdn.qualtrics.com/jfe/static/dist/",
      cbs: [], bootstrapSkin: null, ab: false,
      getPT: function(cb) {
        if (this.pt !== undefined) {
          cb(this.success, this.pt);
        } else {
          this.cbs.push(cb);
        }
      },
      setPT: function(s, pt) {
        this.success = s;
        this.pt = pt;
        if (this.cbs.length) {
          for (var i = 0; i < this.cbs.length; i++) {
            this.cbs[i](s, pt);
          }
        }
      }
    };
  </script>
