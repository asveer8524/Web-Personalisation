            Drupal.FieldGroup.Effects.processAccordion = {
              execute: function (context, settings, type) {
                $("div.field-group-accordion-wrapper", context).once("fieldgroup-effects", function () {
                  var wrapper = $(this);

                  wrapper.accordion({
                    heightStyle: "content",
                    autoHeight: true,
