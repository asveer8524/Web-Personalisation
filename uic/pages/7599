	<div id="tabs">
		<ul class="clearfix" role="tablist">
			<li id="texttab" role="presentation">
<a href="#textcontainer" role="tab" aria-controls="textcontainer" onclick="return showSection('text');">Overview</a></li>
			<li id="undergraduateprogramstexttab" role="presentation">
<a href="#undergraduateprogramstextcontainer" role="tab" aria-controls="undergraduateprogramstextcontainer" onclick="return showSection('undergraduateprogramstext');" >Programs and Requirements</a></li>
			<li id="courseinventorytab" role="presentation"><a href="#courseinventory" role="tab" aria-controls="courseinventorycontainer" onclick="return showSection('courseinventory');">Courses</a></li>
