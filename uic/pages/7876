    <script>
        function resize(){
            var winheight =+ $(window).height();
            var winwidth =+ $(window).width();
            //var winheight = window.innerHeight;
            //var winwidth = window.innerWidth;
            //var testingwidth = $(window).innerWidth();
            var headerhght =+ $('#header').height();
            var footerhght =+ $('#footer').height();
            var wrapperwidth = $('#top').width();
            var containerwidth = $('#container').width();
            var containerheight = winheight - (headerhght + footerhght);

            //Configure Footer placement
            //var containermargin =+ (wrapperwidth - containerwidth)/2;
            //$('#container').css('margin-left', containermargin + 'px');
            //$('#container').css('margin-right', containermargin + 'px');
            //$('#breadcrumb').remove();
            //var html = $('html').width();
            //$('#content h1').html(html);
            $('#container').css('height', containerheight + 'px');
        }
        $(document).load(resize);
        $(window).resize(resize);
    </script>
