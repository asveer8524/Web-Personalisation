      &#42; Set GREEN &#61; &#60;span style&#61;&#34;color: #008000&#34;&#62;
      &#42; Set OLIVE &#61; &#60;span style&#61;&#34;color: #808000&#34;&#62;
      &#42; Set MAROON &#61; &#60;span style&#61;&#34;color: #800000&#34;&#62;
