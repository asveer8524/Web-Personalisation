 <script type="text/javascript">
            var picPaths = ['../../../images/refreshimage/1.jpg','../../../images/refreshimage/2.jpg','../../../images/refreshimage/3.jpg','../../../images/refreshimage/4.jpg','../../../images/refreshimage/5.jpg','../../../images/refreshimage/6.jpg','../../../images/refreshimage/7.jpg','../../../images/refreshimage/8.jpg','../../../images/refreshimage/9.jpg','../../../images/refreshimage/10.jpg','../../../images/refreshimage/11.jpg'];
            var oPics = [];
            for(i=0; i < picPaths.length; i++){
                oPics[i] = new Image();
                oPics[i].src = picPaths[i];
            }
            curPic = Math.floor(Math.random()*oPics.length);
            window.onload=function(){
                document.getElementById('imgRotator').src = oPics[curPic].src;
            }
        </script>

